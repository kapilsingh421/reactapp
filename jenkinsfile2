 node {
      checkout scm
      result= sh (script: "git log -1 | grep 'ci skip'", returnStatus: true)
      result= true
      if (result) {
        echo "performing build..."
      } else {
         echo "Build Aborted"
         currentBuild.result = 'ABORTED'
         error('Stopping earlyâ€¦')
      }
    }
